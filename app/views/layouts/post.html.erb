<!DOCTYPE html>
<html class="ua-ff4" lang="zh-CN">
  <head>
    <title><%= yield :title %></title>
    <%= stylesheet_link_tag "style", "form" %>
    <%= javascript_include_tag "application" %>
    <%= csrf_meta_tags %>
  </head>
  <body>
    <%= render 'shared/top' %>
    <div id='wrapper'>
      <div id='content'>
        <h1><%= yield :h %></h1>
        <div class='grid-20-4 clearfix'>
          <div class='article'>
            <%= yield %>
          </div>
          <div class='aside'>
            <%= render 'shared/side_post' %>
          </div>
        </div>
      </div>
      <%= render 'shared/footer_help' %>
    </div>
  </body>
</html>