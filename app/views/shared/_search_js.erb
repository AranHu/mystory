<script type="text/javascript">(function() {
  this.goSearch = function() {
    return window.location.href = "/s/" + $('#title').val();
  };

  $('#title').on('keypress', function(e) {
    if (e.keyCode == 13) {
      window.location.href = "/s/" + $('#title').val();
    }
  })
}).call(this);
</script>